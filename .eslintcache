({...person, [name]: value})\n        console.log(name, value);\n    };\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        if(person.firstName && person.email && person.age){\n            const newPerson = {...person, id: new Date().getTime().toString()};\n            setPeople([...people, newPerson]);\n            setPerson({firstName: '', email: '', age: ''})\n        }\n    }\n\n    return (\n        <>\n            <article>\n                <form className=\"form\">\n                    <div className='form-control'>\n                        <label htmlFor='firstName'>Name: </label>\n                        <input \n                        type='text' \n                        id='firstName'\n                        name='firstName' \n                        value={person.firstName}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className='form-control'>\n                        <label htmlFor='email'>email: </label>\n                        <input \n                        type='text' \n                        id='email' \n                        name='email' \n                        value={person.email}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className='form-control'>\n                        <label htmlFor='age'>Age: </label>\n                        <input \n                        type='text' \n                        id='age' \n                        name='age' \n                        value={person.age}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <button onClick={handleSubmit} type='submit' className='btn'>Add person</button>\n                </form>\n\n                {\n                    people.map( (person, index) => {\n                        const {id, firstName, email, age} = person;\n                        return (\n                            <div className='item' key={id}>\n                                <h4>{ firstName }</h4>\n                                <p>{ email }</p>\n                                <p>{ age }</p>\n                            </div>\n                        );\n                    })\n                }\n            </article>\n        </>\n    )\n}\n\nexport default MultipleInputs;","/home/karigor/reactjs/react-advence/src/tutorial/5-useRef/setup/1-useRef-basics.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/index.js",["105"],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/Modal.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/reducer.js",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":4,"column":12,"nodeType":"112","messageId":"113","endLine":4,"endColumn":18},{"ruleId":"110","severity":1,"message":"114","line":4,"column":20,"nodeType":"112","messageId":"113","endLine":4,"endColumn":29},{"ruleId":"110","severity":1,"message":"115","line":8,"column":18,"nodeType":"112","messageId":"113","endLine":8,"endColumn":25},{"ruleId":"110","severity":1,"message":"116","line":9,"column":17,"nodeType":"112","messageId":"113","endLine":9,"endColumn":23},{"ruleId":"106","replacedBy":"117"},{"ruleId":"108","replacedBy":"118"},{"ruleId":"106","replacedBy":"119"},{"ruleId":"108","replacedBy":"120"},{"ruleId":"106","replacedBy":"121"},{"ruleId":"108","replacedBy":"122"},{"ruleId":"110","severity":1,"message":"123","line":7,"column":18,"nodeType":"112","messageId":"113","endLine":7,"endColumn":25},{"ruleId":"110","severity":1,"message":"124","line":1,"column":27,"nodeType":"112","messageId":"113","endLine":1,"endColumn":36},{"ruleId":"110","severity":1,"message":"124","line":1,"column":39,"nodeType":"112","messageId":"113","endLine":1,"endColumn":48},"no-native-re