-advence/src/tutorial/3-conditional-rendering/setup/1-multiple-return.js",[],["175","176"],"/home/karigor/reactjs/react-advence/src/tutorial/3-conditional-rendering/setup/2-short-circuit.js",["177"],"/home/karigor/reactjs/react-advence/src/tutorial/3-conditional-rendering/setup/3-show-hide.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/4-forms/setup/1-controlled-inputs.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/4-forms/setup/2-multiple-inputs.js",["178"],"import React, { useState, useEffect } from 'react';\n\nconst MultipleInputs = () => {\n    // const [firstName, setFirstName] = useState('');\n    // const [email, setEmail] = useState('');\n    // const [age, setAge] = useState('');\n    const [person, setPerson] = useState({\n        firstName: '',\n        email: '',\n        age: ''\n    });\n    const [people, setPeople] = useState([]);\n    \n    const handleChange = (e) => {\n        const name = e.target.name;\n        const value = e.target.value;\n        setPerson({...person, [name]: value})\n        console.log(name, value);\n    };\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        if(person.firstName && person.email && person.age){\n            const newPerson = {...person, id: new Date().getTime().toString()};\n            setPeople([...people, newPerson]);\n            setPerson({firstName: '', email: '', age: ''})\n        }\n    }\n\n    return (\n        <>\n            <article>\n                <form className=\"form\">\n                    <div className='form-control'>\n                        <label htmlFor='firstName'>Name: </label>\n                        <input \n                        type='text' \n                        id='firstName'\n                        name='firstName' \n                        value={person.firstName}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className='form-control'>\n                        <label htmlFor='email'>email: </label>\n                        <input \n                        type='text' \n                        id='email' \n                        name='email' \n                        value={person.email}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className='form-control'>\n                        <label htmlFor='age'>Age: </label>\n                        <input \n                        type='text' \n                        id='age' \n                        name='age' \n                        value={person.age}\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <button onClick={handleSubmit} type='submit' className='btn'>Add person</button>\n                </form>\n\n                {\n                    people.map( (person, index) => {\n                        const {id, firstName, email, age} = person;\n                        return (\n                            <div className='item' key={id}>\n                                <h4>{ firstName }</h4>\n                                <p>{ email }</p>\n                                <p>{ age }</p>\n                            </div>\n                        );\n                    })\n                }\n            </article>\n        </>\n    )\n}\n\nexport default MultipleInputs;",["179","180"],"/home/karigor/reactjs/react-advence/src/tutorial/5-useRef/setup/1-useRef-basics.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/index.js",["181"],"import React, { useState, useReducer, useEffect } from 'react';\nimport Modal from './Modal';\nimport { reducer } from './reducer';\n\n\nconst defaultState = {\n    people: [],\n    isModalOpen: false,\n    modalContent: ''\n}\n\nconst Index = () => {\n    const [name, setName] = useState('');\n    const [state, dispatch] = useReducer(reducer,defaultState);\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        if(name){\n            const newItem = {id: new Date().getTime().toString(), name};\n            dispatch({type: 'ADD_ITEM', payload: newItem });\n        }else{\n            dispatch({type: 'NO_VALUE'});\n        }\n\n    }\n\n    const closeModal = () => {\n        dispatch({ type: 'CLOSE_MODAL' })\n    }\n\n    return (\n        <>\n            {state.isModalOpen && <Modal closeModal={closeModal} modalContent={state.modalContent} />}\n            <form onSubmit={handleSubmit} className='form'>\n                <div>\n                    <input \n                    type=\"text\"\n                    value={ name }\n                    onChange={ (e) => setName(e.target.value)}\n                    />\n                </div>\n                <button type='submit'>add</button>\n            </form>\n            {state.people.map((person) => {\n                return (\n                <div key={person.id} className='item'>\n                    <h4>{person.name}</h4>\n                    <button \n                    onClick={ () => \n                    dispatch({ type: 'REMOVE_ITEM', payload: person.id })}>\n                        remove</button>\n                </div>\n                );\n            })}\n        </>\n    )\n}\n\n\nexport default Index;",["182","183"],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/Modal.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/6-useReducer/setup/reducer.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/7-prop-drilling/setup/1-prop-drilling.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/8-useContext/setup/1-context-api.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/9-custom-hooks/setup/1-fetch-example.js",["184","185"],"/home/karigor/reactjs/react-advence/src/tutorial/9-custom-hooks/setup/2-usefetch.js",["186"],"/home/karigor/reactjs/react-advence/src/tutorial/10-prop-types/setup/index.js",[],["187","188"],"/home/karigor/reactjs/react-advence/src/tutorial/10-prop-types/setup/product.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/10-prop-types/setup/useFetch.js",["189"],"import { useState, useEffect } from 'react';\n\n\nexport const useFetch = (url) => {\n    const [loading, setLoading] = useState(true);\n    const [products, setProducts] = useState([]);\n    \n    const getProducts = async () => {\n        const response = await fetch(url);\n        const products = await response.json();\n        setProducts(products);\n        setLoading(false);\n    }\n\n    useEffect( () => {\n        getProducts();\n    }, [url]);\n\n    return {loading, products};\n}","/home/karigor/reactjs/react-advence/src/tutorial/9-custom-hooks/final/2-usefetch.js",["190"],"import {useState, useEffect } from 'react';\n\nexport const useFetch = (url) => {\n    const [loading, setLoading] = useState(true);\n    const [products, setProducts] = useState([]);\n\n    const getProducts = async () => {\n        const response = await fetch(url);\n        const products = await response.json();\n        setProducts(products);\n        setLoading(false);\n    }\n\n    useEffect(() => {\n        getProducts();\n    }, [url]);\n\n    return [ loading, products ]\n};","/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/index.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/About.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/Error.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/Person.js",["191"],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/Navbar.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/Home.js",[],"/home/karigor/reactjs/react-advence/src/tutorial/11-react-router/setup/People.js",["192"],"/home/karigor/reactjs/react-advence/src/tutorial/12-memo-useMemo-useCallback/setup/index.js",[],"/